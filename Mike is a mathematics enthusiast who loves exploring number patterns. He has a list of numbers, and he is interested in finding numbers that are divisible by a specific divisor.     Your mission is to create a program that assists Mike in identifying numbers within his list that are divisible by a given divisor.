#include <iostream>
using namespace std;

int main() {
    int n, divisor;
    cin >> n;
    cin >> divisor;

    int mikeList[n];

    for (int i = 0; i < n; i++) {
        cin >> mikeList[i];
    }

    for (int i = 0; i < n; i++) {
        if (mikeList[i] % divisor == 0) {
            cout << mikeList[i] << " ";
        }
    }

    return 0;
}
